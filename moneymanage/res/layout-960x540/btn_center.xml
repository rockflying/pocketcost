<?xml version="1.0" encoding="UTF-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_enabled="true" android:state_pressed="false">
        <shape> 
            <solid android:color="#748793"/> 
         </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape> 
             <solid android:color="#435435"/> 
        </shape>
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <shape> 
              <solid android:color="#437435"/> 
        </shape>
    </item>
  
</selector>